void loop()
{

    float value = distanceSensor.measureDistanceCm();
    int logic = 0;

    if (!AI.invoke())
    {
        Serial.println("invoke success");
        Serial.print("perf: prepocess=");
        Serial.print(AI.perf().prepocess);
        Serial.print(", inference=");
        Serial.print(AI.perf().inference);
        Serial.print(", postpocess=");
        Serial.println(AI.perf().postprocess);

        pred_index = AI.classes()[0].target;

        if(value > 10 && value <= 22) {
            logic +=1;
            if( pred_index == 0 && logic == 1) {
                display.clearDisplay();
                display.setCursor(0, 0);
                display.drawBitmap(0, 0,  Anorganik, 128, 64, WHITE);
                display.display(); 

                servo1.write(0);
                
                delay(2000);
            } else if(pred_index == 1 && logic == 1) {
                display.clearDisplay();
                display.setCursor(0, 0);
                display.drawBitmap(0, 0,  Organik, 128, 64, WHITE);
                display.display(); 
                
                servo1.write(180);
                delay(2000);
            }
        } else {
            logic = 0;
            if(pred_index == 0 && logic == 0){
                display.clearDisplay();
                display.display(); 
                servo1.write(88);
               
            } else if (pred_index == 1 && logic == 0) {
                display.clearDisplay();
                display.display(); 
                servo1.write(88); 
                
            }

        }

        Serial.print("Result= Label: ");
        Serial.print(pred_index);
        Serial.print(", score=");
        Serial.println(AI.classes()[0].score);
    }

    

}